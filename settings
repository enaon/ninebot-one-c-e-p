//info face
face[5] = {
  offms: 3000,
  init: function(){
    var g=w.gfx;
	var mem=process.memory();
	var s=(getTime()-set.boot)|0;
	var d=0;
	var h=0;
	var m=0;
	if (s>864000) {set.boot=getTime();s=(getTime()-set.boot)|0;}
	while (s>86400) {s=s-86400;d++;}
	while (s>3600) {s=s-3600;h++;}
	while (s>60) {s=s-60;m++;}
    g.setColor(0,col.black);
    g.fillRect(0,0,239,239); //all
    g.setColor(1,colo.txt1);
    g.setFont("Vector",18);
	g.drawString("MEM FREE: "+mem.free+"/"+mem.total,120-(g.stringWidth("MEM FREE: "+mem.free+"/"+mem.total)/2),0);  
	g.drawString("VERSION: "+process.version,120-(g.stringWidth("VERSION: "+process.version)/2),25);  
	g.drawString("ACC TYPE: "+acctype,120-(g.stringWidth("ACC TYPE: "+acctype)/2),50);  
    g.drawString("TOUCH TYPE: "+touchtype,120-(g.stringWidth("TOUCH TYPE: "+touchtype)/2),75);  
    g.drawString("UPTIME: "+d+"D-"+h+"H-"+m+"M",120-(g.stringWidth("UPTIME: "+d+"D-"+h+"H-"+m+"M")/2),100);  
	g.drawString("FLASH FREE: "+require("Storage").getFree(),120-(g.stringWidth("FLASH FREE: "+require("Storage").getFree())/2),125); 
	g.drawString("TEMPERATURE: "+E.getTemperature(),120-(g.stringWidth("TEMPERATURE: "+E.getTemperature())/2),150);  
	g.drawString("NAME: "+set.name,120-(g.stringWidth("NAME: "+set.name)/2),175);  
    g.flip();
    g.setFont("Vector",18);
	g.setColor(0,col.raf);
	g.fillRect(0,200,115,239);
	g.setColor(1,col.white);
	g.drawString("RESTART",15,213);
    g.flip();	
	g.setColor(0,col.raf);
	g.fillRect(125,200,239,239);
	g.setColor(1,col.white);
	g.drawString("SHUTDOWN",132,213);
	g.flip();
    face[0].appImgNone=0;
  },
  show : function(){
	return;
  },
  clear : function(o){
    var g=w.gfx;
    pal[0]=colo.bck0;
    g.clear();
    this.exit(o);
    return true;
  },
  exit: function(){
    this.run=false;
    if (this.tid>=0) clearTimeout(this.tid);
    this.tid=-1;
    return true;
  },
  off: function(o){
    var g=w.gfx;
    g.off();
    this.clear(o);
  }
};
//touch-settings  
touchHandler[0]=function(e,x,y){
    var g=w.gfx;
    var p=w.pin.MOTOR;
    if (set.tor==1){
        g.bri.set(face[0].cbri);
        set.tor=-1;
        face[0].tor=0;
        face.go("settings",0);
        return;
    }else if (e==5){
      if(x<77&&y<75){//btn1
        if (face[0].btSet) {
          digitalPulse(p,1,[30,50,30]);face[0].btSet=0;
          face[0].gb=-1;face[0].cli=-1;face[0].bt=-1;face[0].hid=-1;face[0].atc=-1;face[0].bri=-1;face[0].acc=-1;face[0].dnd=-1;face[0].sys=1;this.btn2=1;
        }else if (face.mode) {if (face[0].appDo1) {digitalPulse(p,1,[30,50,30]);eval(face[0].appDo1);return;} else digitalPulse(p,1,40);
        }else {face[0].btSetOn=1;face[0].btSet=1;digitalPulse(p,1,[30,50,30]);
        face[0].gb=-1;face[0].cli=-1;face[0].bt=-1;face[0].hid=-1;face[0].atc=-1;face[0].bri=-1;face[0].acc=-1;face[0].dnd=-1;face[0].sys=1;face[0].btn2=1;
        }
        //{set.atc=1-set.atc;set.upd();digitalPulse(p,1,[30,50,30]);}
      }else if(77<x&&x<158&&y<75){//btn2
        if (face[0].btSet) {
          digitalPulse(p,1,[30,50,30]);face[0].btSet=0;
          face[0].gb=-1;face[0].cli=-1;face[0].bt=-1;face[0].hid=-1;face[0].atc=-1;face[0].bri=-1;face[0].acc=-1;face[0].dnd=-1;face[0].sys=1;face[0].btn2=1;
        }else if (face.mode) {if (face[0].appDo2) {digitalPulse(p,1,[30,50,30]);eval(face[0].appDo2);return;} else digitalPulse(p,1,40);
        }else if (face[0].btSet) {
          digitalPulse(p,1,40);
        }else digitalPulse(p,1,40);
      }else if(158<x&&x<239&&y<75){//btn3
        if (face.mode) {if (face[0].appDo3) {digitalPulse(p,1,[30,50,30]);eval(face[0].appDo3);return;} else digitalPulse(p,1,40);
        }else if (face[0].btSet) {
          set.cli=1-set.cli;set.upd();digitalPulse(p,1,[30,50,30]);
        }else {set.dnd=1-set.dnd;digitalPulse(p,1,[30,50,30]);}
      }else if(77>x&&77<y&&y<159){//btn4
        if (face.mode) {if (face[0].appDo4) {digitalPulse(p,1,[30,50,30]);eval(face[0].appDo4);return;} else digitalPulse(p,1,40);
        }else if (face[0].btSet) {
          set.gb=1-set.gb;digitalPulse(p,1,[30,50,30]);
        }else {digitalPulse(p,1,[30,50,30]);face.go("settings",5);return;}
      }else if(77<x&&x<157&&77<y&&y<159){//btn5
        if (face.mode) {if (face[0].appDo5) {digitalPulse(p,1,[30,50,30]);eval(face[0].appDo5);return;} else digitalPulse(p,1,40);
        }else if (face[0].btSet) {
          set.atc=1-set.atc;set.upd();digitalPulse(p,1,[30,50,30]);
        }else {set.acc=1-set.acc;set.accR();digitalPulse(p,1,[30,50,30]);}
      }else if(158<x&&x<239&&77<y&&y<159) {//btn6
        if (face.mode) {if (face[0].appDo6) {digitalPulse(p,1,[30,50,30]);eval(face[0].appDo6);return;} else digitalPulse(p,1,40);
        }else if (face[0].btSet) {
          set.hid=1-set.hid;set.upd();digitalPulse(p,1,[30,50,30]);
        }else {
        face[0].cbri=g.bri.lv+1;
        if (face[0].cbri>7) face[0].cbri=1;
        g.bri.set(face[0].cbri);   
   		digitalPulse(p,1,[30,50,30]);
        }
      }else if(0<x&&x<75&&158<y&&y<239){
        set.tor=1;
        face[0].cbri=w.gfx.bri.lv;
        g.bri.set(7);
        face[0].tor=1;
        if (face.offid>=0) {clearTimeout(face.offid); face.offid=-1;}
        face.offid=setTimeout((f)=>{
		  face[0].tor=0;
          set.tor=-1;
          g.bri.set(face[0].cbri);
		  if (f>=0 && face[f].off) face[f].off();
          face.offid=-1;face.pageCurr=-1;face.appPrev="main";
        },25000,face.pageCurr);
   		digitalPulse(p,1,[30,50,30]);
        return;  
      }else if(77<x&&x<157&&158<y&&y<239){	
   		digitalPulse(p,1,[30,50,30]);
		face.go("settings",5);return;
      }else if(158<x&&x<239&&160<y&&y<239){
		digitalPulse(p,1,[30,50,30]);
		if (Boolean(require("Storage").read("calc"))) {
          face.go("calc",0);return;}
      } else digitalPulse(p,1,40);
    }else if  (e==1){
      face[0].btSet=0;
      if(158<x&&x<239&&77<y&&y<160) {
        face[0].cbri=g.bri.lv-1;
        if (face[0].cbri<1) face[0].cbri=1;
        g.bri.set(face[0].cbri);
   		digitalPulse(p,1,[30,50,30]);
      }else { 
        if (face.faceSave!=-1) {
          face.go(face.faceSave[0],face.faceSave[1],face.faceSave[2]);face.faceSave=-1;
        }else{
          if (face.appPrev=="settings") {face.appPrev="main";face.pagePrev=0;}
		  face.go(face.appPrev,face.pagePrev,face.pageArg);return;
        }
      }  
    }else if  (e==2){
      if(158<x&&x<239&&77<y&&y<160&&!face.mode) {
        face[0].cbri=g.bri.lv+1;
        if (face[0].cbri>7) face[0].cbri=7;
        g.bri.set(face[0].cbri);
   		digitalPulse(p,1,[30,50,30]);
      }else if (Boolean(require('Storage').read('w_apps'))){
        face.mode=1-face.mode;
        face[0].btSet=0;
        face[0].gb=-1;face[0].cli=-1;face[0].bt=-1;face[0].hid=-1;face[0].atc=-1;face[0].bri=-1;face[0].acc=-1;face[0].dnd=-1;face[0].sys=1;face[0].btn2=1;
   		digitalPulse(p,1,[30,50,30]);
      } else digitalPulse(p,1,40);
    }else if  (e==3){
	  digitalPulse(p,1,40);
    }else if  (e==4){
      if (face[0].btSet) {
        face[0].btSet=0;
        face[0].gb=-1;face[0].cli=-1;face[0].bt=-1;face[0].hid=-1;face[0].atc=-1;face[0].bri=-1;face[0].acc=-1;face[0].dnd=-1;face[0].sys=1;face[0].btn2=1;
        
      }else if (face.faceSave!=-1) {
          face.go(face.faceSave[0],face.faceSave[1],face.faceSave[2]);face.faceSave=-1;
      }else{
          if (face.appPrev=="settings") {face.appPrev="main";face.pagePrev=0;}
		  face.go(face.appPrev,face.pagePrev,face.pageArg);return;
      }
    }else if  (e==12){
      if(158<x&&x<239&&y<75){
        set.hid=1-set.hid;
		set.upd();
        digitalPulse(p,1,[30,50,30]);
      }
    }
   this.timeout();
};
//
touchHandler[5]=function(e,x,y){
    var p=w.pin.MOTOR;
    if (e==5){
      if (x<120 && y>190) {
		NRF.removeListener('disconnect',bdis);  
        NRF.disconnect(); 
	    reset();
	  } else  digitalPulse(p,1,40);
    }else if  (e==1){
	  face.go("settings",0);return;
    }else if  (e==2){
	  if (y>160&&x<50) {
        if (w.gfx.bri.lv!==7) {this.bri=w.gfx.bri.lv;w.gfx.bri.set(7);}
        else w.gfx.bri.set(this.bri);
		digitalPulse(p,1,[30,50,30]);
	  } else digitalPulse(p,1,40);
    }else if  (e==3){
      digitalPulse(p,1,40);
    }else if  (e==4){
	  face.go("settings",0);return;
    }else if  (e==12){
	//devmode-shutdown
	 if (x<120 && y>190) {
       require("Storage").write("devmode","dev");
       E.reboot();}
	 else if (x>120 && y>190) {
       NRF.disconnect();
       require("Storage").write("devmode","off");
       E.reboot();}
     else digitalPulse(p,1,40);
    }
   this.timeout();
};
