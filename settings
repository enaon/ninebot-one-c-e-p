    g.off();
    this.clear(o);
  },
   btn:function(state,rectx0,recty0,rectx1,recty1,Img,ImgX,ImgY,fCol,bCol){
      var g=w.gfx;
      this.colf=(fCol)?fCol:col.white;
      this.colb=(bCol)?bCol:col.gray;
      if (state===1) {this.colb=col.raf1;}
      else {this.colb=col.gray;this.colf=col.black;}
      g.setColor(1,this.colb);
      g.fillRect(rectx0,recty0,rectx1,recty1);
      g.setColor(0,this.colf);
      g.drawImage(Img,ImgX,ImgY);
      g.flip();
  },
};

